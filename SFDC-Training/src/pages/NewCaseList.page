<apex:page controller="NewCaseListController">
    <apex:form >
        <apex:pageBlock title="Cases List" id="cases_list">
            <apex:repeat value="{!newCases}" var="case" id="theRepeat">
                <li>
                    <apex:outputLink value="/{! case.id }" >{! case.CaseNumber }</apex:outputLink>
                </li>
            </apex:repeat>        
        </apex:pageBlock>
    </apex:form>
</apex:page>